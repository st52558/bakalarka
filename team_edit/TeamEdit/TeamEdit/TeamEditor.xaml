<Window x:Class="TeamEdit.TeamEditor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeamEdit"
        mc:Ignorable="d"
        Title="TeamEditor" Height="700" Width="600">
    <Grid>
        <Image Visibility="Visible" Name="TeamLogo" HorizontalAlignment="Left" Height="250" Margin="10,90,0,0" VerticalAlignment="Top" Width="250" RenderTransformOrigin="0.5,0.5"/>
        <TextBox Name="NameBox" HorizontalAlignment="Left" Height="30" Margin="65,30,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="195"/>
        <Label Name="NameLabel" Content="Name" Height="30" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top"/>
        <Label Name="ShortcutLabel" Content="Shortcut" Height="30" HorizontalAlignment="Left" Margin="10,370,0,0" VerticalAlignment="Top"/>
        <TextBox Name="ShortcutBox" HorizontalAlignment="Left" Height="30" Margin="65,370,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="195"/>

    </Grid>
</Window>
