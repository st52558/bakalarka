<Window x:Class="EsportManager.Traveling"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EsportManager"
        mc:Ignorable="d"
        Title="Traveling" Height="165" Width="230" WindowStartupLocation="CenterScreen">
    <Grid>
        <ComboBox Name="SectionsCB" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="195" SelectionChanged="SectionChange" />
        <Button Name="GetHome" Content="Poslat domů" HorizontalAlignment="Left" Margin="130,97,0,0" VerticalAlignment="Top" Width="75" Click="MovePlayersHome" />
        <Button Name="Move" Content="Přemístit" HorizontalAlignment="Left" Margin="10,97,0,0" VerticalAlignment="Top" Width="75" Click="MovePlayers" />
        <ComboBox Name="CitiesCB" HorizontalAlignment="Left" Margin="10,54,0,0" VerticalAlignment="Top" Width="195" SelectionChanged="CityChange" />

    </Grid>
</Window>
